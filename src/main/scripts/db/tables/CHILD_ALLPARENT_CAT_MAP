USE PRODUCTS;
DROP TABLE IF EXISTS CHILD_ALLPARENT_CAT_MAP;
CREATE TABLE CHILD_ALLPARENT_CAT_MAP (
  `CATEGORY_ID` int(11) NOT NULL DEFAULT '0',
  `PARENT_CATEGORY_ID` int(11) NOT NULL DEFAULT '0',
  `TIME_MODIFIED` datetime NOT NULL,
  PRIMARY KEY (`CATEGORY_ID`,`PARENT_CATEGORY_ID`) USING HASH
) ENGINE=MyISAM 
